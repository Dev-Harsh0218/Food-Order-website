<div id="login-head">
    <div id="login-head-text">
        <h1>Tell us Who you are</h1>
        <!-- <h1>Welcome Back !</h1> -->
    </div>
    <img src="img/back7.jpg" alt="">
</div>


<div id="register_form_con">
    <div id="side_img">
        <img src="img/register_pic.jpg" alt="">
    </div>
    <div id="actu-form-con">
        <h2>Register</h2>
        <form action="/login" id="actu-form" method="post">
            <div class="input-fields">
                 <label for="Name">full name</label>
                 <input type="text" id="input_email">
            </div>
            <div class="input-fields">
                <label for="Username_email">Username</label>
                <input type="email" id="input_email">
            </div>
            <div class="input-fields">
                 <label for="password">password</label>
                 <input type="password" id="passwd">
            </div>
            <div class="input-fields">
                <label for="phone">phone no.</label>
                <input type="text" id="phone">
           </div>
            <button type="submit" id="sign_in_btn">I want in</button>
            <div id="regi-link">
                <p>Already have an account ? <a href="/login">Sign In</a></p>
            </div>
        </form>
    </div>
</div>










